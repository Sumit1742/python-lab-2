row = 10
for i in range(1,row+1):
    if i == 1 :
        print("   &"+"  &")
    elif i == row-3:
        print(" &   "+"   "+" & ")
    elif i == row-2:
        print("&  "+"       "+"& ")
for i in range(row+1,0,-1):
     if i == row-2:
         print("              &  "+"       "+"& ")
     elif i == row-3:
         print("               &   "+"   "+" & ")
     elif i == 1 :
         print("                  &"+"  &")
